{
  "files.exclude": {
    "tsc/**/config.js": true,
    "**/logs": true,
    "**/*.log": true,
    "**/npm-debug.log*": true,
    "**/yarn-debug.log*": true,
    "**/yarn-error.log*": true,
    "**/lerna-debug.log*": true,
    "**/report.[0-9]*.[0-9]*.[0-9]*.[0-9]*.json": true,
    "**/pids": true,
    "**/*.pid": true,
    "**/*.seed": true,
    "**/*.pid.lock": true,
    "**/lib-cov": true,
    "**/coverage": true,
    "**/*.lcov": true,
    "**/.nyc_output": true,
    "**/.grunt": true,
    "**/bower_components": true,
    "**/.lock-wscript": true,
    "build/Release": true,
    "**/node_modules/": true,
    "**/jspm_packages/": true,
    "**/web_modules/": true,
    "**/*.tsbuildinfo": true,
    "**/.npm": true,
    "**/.eslintcache": true,
    "**/.rpt2_cache/": true,
    "**/.rts2_cache_cjs/": true,
    "**/.rts2_cache_es/": true,
    "**/.rts2_cache_umd/": true,
    "**/.node_repl_history": true,
    "**/*.tgz": true,
    "**/.yarn-integrity": true,
    "**/.env": true,
    "**/.env.test": true,
    "**/.cache": true,
    "**/.parcel-cache": true,
    "**/.next": true,
    "**/out": true,
    "**/.nuxt": true,
    "**/dist": true,
    "**/.cache/": true,
    ".vuepress/dist": true,
    "**/.serverless/": true,
    "**/.fusebox/": true,
    "**/.dynamodb/": true,
    "**/.tern-port": true,
    "**/.vscode-test": true,
    ".yarn/cache": true,
    ".yarn/unplugged": true,
    ".yarn/build-state.yml": true,
    "**/.pnp.*": true,
    "**/tsconfig.json": true
  },
  "separators.functions.borderStyle": "solid",
  "separators.functions.ignoreCallbackInline": true,
  "workbench.colorCustomizations": {
    "separators.classes.borderColor": "#6b17cc",
    "separators.constructors.borderColor": "#a6c942",
    "separators.enums.borderColor": "#ffd966",
    "separators.functions.borderColor": "#3c4262",
    "separators.interfaces.borderColor": "#ff6d01",
    "separators.methods.borderColor": "#3c4262",
    "separators.namespaces.borderColor": "#4f8a35"
  }
}